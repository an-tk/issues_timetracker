<% unless projects.empty? %>
    <ul class="nav nav-pills nav-stacked">
      <li class="nav-header">My projects</li>
      <% projects.each do |project|%>
          <li>
            <%= link_to project.title, project %>
          </li>
      <% end %>
      <li>
        <%= link_to '+ Project', new_project_path, :class => "muted" %>
      </li>
    </ul>
<% end %>


